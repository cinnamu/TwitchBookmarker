{
    "name": "Twitch Chat Bookmarker",
    "description": "This extension will allow you to bookmark chat messages so that when you focus on your game and come back you can start back reading where you left off!;",
    "version": "1.0",
    "manifest_version": 2,
    "icons": {
            "16": "16.png",
            "32": "32.png",
            "128": "128.png"
    },
    "background": {
        "scripts": ["background.js"]
        },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "16.png",
            "32": "32.png",
            "128": "128.png"
        }
    },
    "options_page": "options.html",
    "permissions": [
        "tabs",
        "*://*/*"
    ]
}

